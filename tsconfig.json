{
    "compilerOptions": {
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "skipLibCheck": true,
        "lib": ["es2015", "dom"],
        "typeRoots": [
            "./node_modules/@types",
            "./node_modules"
        ],
        "types": [
            "node"
        ],
        "noUnusedLocals": true,
        "noUnusedParameters": true
    }
}
