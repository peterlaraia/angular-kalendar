{
  "name": "angular-kalendar",
  "version": "2.0.0",
  "private": true,
  "description": "Angular 2 Datetime Picker/Calendar",
  "main": "bundles/angular-kalendar.umd.js",
  "module": "angular-kalendar.es5.js",
  "es2015": "angular-kalendar.js",
  "types": "angular-kalendar.d.ts",
  "scripts": {
    "precommit": "npm run lint",
    "lint": "tslint src/**/*.ts",
    "lint:fix": "tslint src/**/*.ts test/**/*.ts --fix",
    "test": "mocha --require ts-node/register test/**/*.spec.ts --recursive",
    "test:watch": "mocha -w --require ts-node/register test/**/*.spec.ts --recursive",
    "clean:build": "rimraf build",
    "clean": "rimraf dist build",
    "copy:metadata": "ts-node tools/utils/cp-metadata.ts",
    "transpile:es6": "ngc -p tsconfig-build.json",
    "transpile:es5": "ngc -p tsconfig-build.es5.json",
    "rollup:es6": "rollup -c tools/config/rollup/rollup.es6.config.js",
    "rollup:es5": "rollup -c tools/config/rollup/rollup.es5.config.js",
    "rollup:umd": "rollup -c tools/config/rollup/rollup.umd.config.js",
    "minify:umd": "uglifyjs dist/bundles/angular-kalendar.umd.js -c -m -o dist/bundles/angular-kalendar.umd.min.js",
    "build:es6": "npm run clean:build && npm run transpile:es6 && npm run rollup:es6 && npm run copy:metadata",
    "build:es5": "npm run clean:build && npm run transpile:es5 && npm run rollup:es5",
    "build:umd": "npm run rollup:umd && npm run minify:umd",
    "prebuild": "npm run clean && npm run lint",
    "build": "npm run build:es6 && npm run build:es5 && npm run build:umd",
    "postbuild": "ts-node tools/utils/post-build.ts",
    "prebuild.demo": "ts-node tools/utils/build-demo.ts",
    "build.demo": "rimraf demo/dist && tsc -p tsconfig-demo.json",
    "serve.demo": "node tools/servers/demo.js",
    "predemo": "npm run build.demo",
    "demo": "npm run serve.demo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterlaraia/angular-kalendar.git"
  },
  "keywords": [
    "angular2",
    "angular",
    "calendar",
    "kalendar",
    "datetime",
    "picker"
  ],
  "author": "Peter Laraia <PeterLaraiaGit@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/peterlaraia/angular-kalendar/issues"
  },
  "homepage": "https://github.com/peterlaraia/angular-kalendar#readme",
  "peerDependencies": {
    "@angular/core": ">=2.3.1",
    "@angular/common": ">=2.3.1",
    "@angular/forms": ">=2.3.1"
  },
  "devDependencies": {
    "@angular/common": "^4.0.1",
    "@angular/compiler": "^4.0.1",
    "@angular/compiler-cli": "^4.0.1",
    "@angular/core": "^4.0.1",
    "@angular/forms": "^4.0.1",
    "@angular/platform-browser": "^4.0.1",
    "@angular/platform-browser-dynamic": "^4.0.1",
    "@types/chai": "^3.4.34",
    "@types/lodash": "4.14.52",
    "@types/mocha": "^2.2.37",
    "@types/node": "^7.0.0",
    "chai": "^3.5.0",
    "codelyzer": "^3.0.0",
    "connect": "^3.6.0",
    "core-js": "^2.4.1",
    "express": "^4.15.2",
    "fs-extra": "^2.1.2",
    "husky": "^0.13.3",
    "lodash": "4.17.4",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "open": "0.0.5",
    "path": "^0.12.7",
    "recursive-copy": "^2.0.6",
    "reflect-metadata": "^0.1.9",
    "rimraf": "^2.5.4",
    "rollup": "^0.41.4",
    "rxjs": "^5.0.3",
    "serve-static": "^1.11.2",
    "systemjs": "0.19.28",
    "ts-helpers": "^1.1.2",
    "ts-node": "^2.0.0",
    "tslib": "^1.5.0",
    "tslint": "^5.1.0",
    "typescript": "^2.1.4",
    "uglify-js": "^2.7.5",
    "zone.js": "^0.8.4"
  }
}
