{
  "name": "angular2-datetime",
  "version": "0.1.0",
  "description": "angular2 datetime picker",
  "scripts": {
    "lint": "tslint -c tslint.json src/**/*.ts test/**/*.ts",
    "test": "mocha --require ts-node/register test/**/*.spec.ts --recursive",
    "test:watch": "mocha -w --require ts-node/register test/**/*.spec.ts --recursive",
    "distclean": "rimraf dist",
    "transpile": "tsc -p tsconfig-esm.json",
    "package": "rollup -c rollup.config.js dist/angular2-datetime.module.js > dist/angular2-datetime.bundle.js",
    "finish": "cp package.json dist && ngc && cp README.md dist",
    "build": "npm run distclean && npm run transpile && npm run package && npm run finish",
    "build.demo": "rimraf demo-internal/dist demo-internal/ng2datetime/* && tsc -p tsconfig-demo.json && cp dist/* demo-internal/ng2datetime -r"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterlaraia/angular2-datetime.git"
  },
  "keywords": [
    "angular2",
    "datetime",
    "picker",
    "angular"
  ],
  "author": "Peter Laraia <PeterLaraiaGit@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/peterlaraia/angular2-datetime/issues"
  },
  "homepage": "https://github.com/peterlaraia/angular2-datetime#readme",
  "peerDependencies": {
    "@angular/core": ">=2.0.0"
  },
  "devDependencies": {
    "@angular/common": "^2.4.2",
    "@angular/compiler": "^2.4.2",
    "@angular/compiler-cli": "^2.4.2",
    "@angular/core": "^2.4.2",
    "@angular/forms": "^2.4.2",
    "@angular/platform-browser": "^2.4.2",
    "@angular/platform-browser-dynamic": "^2.4.2",
    "@types/chai": "^3.4.34",
    "@types/lodash": "4.14.52",
    "@types/mocha": "^2.2.37",
    "@types/node": "^7.0.0",
    "chai": "^3.5.0",
    "codelyzer": "^2.0.0-beta.4",
    "core-js": "^2.4.1",
    "lodash": "4.17.4",
    "mocha": "^3.2.0",
    "reflect-metadata": "^0.1.9",
    "rimraf": "^2.5.4",
    "rollup": "^0.41.4",
    "rxjs": "^5.0.3",
    "systemjs": "0.19.28",
    "ts-helpers": "^1.1.2",
    "ts-node": "^2.0.0",
    "tslib": "^1.5.0",
    "tslint": "^4.3.1",
    "typescript": "^2.1.4",
    "uglify-js": "^2.7.5",
    "zone.js": "^0.7.4"
  }
}
